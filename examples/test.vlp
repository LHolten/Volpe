a = 3
b = 5

fizzbuzz = n => {
    n % (a * b) == 0 -> :> -3 # fizzbuzz
    n % a == 0 -> :> -2 # fizz
    n % b == 0 -> :> -1 # buzz
    :> n
}

fb_100 = 0..100 :: fizzbuzz

test = ((0..10 :: x => {
    x > 5 -> :> x.0
    :> x.5
}) :: x => {
    x < 2.0 -> :> ~(1.5 * x)
    :> ~(x / 2.0)
}) :: x => {
    x > 1 -> :> 0..x
    :> 0..10
}

:> test
