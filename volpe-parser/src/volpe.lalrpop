use std::str::FromStr;

grammar;

pub Block = (App "=" App ";")* <App>;

List = (List2 ";")* <List2>;

List2 = (App ",")* <App>;

Obj = (Ident ":" App ";")* Ident ":" <App>;

App = Term* <Term>;

Term = {
    Num,
    Ident,
    "(" <Block> ")",
    "[" <List> "]",
    "{" <Obj> "}"
};


Ident = r"[_a-zA-Z][_a-zA-Z0-9]*";

Num = r"[0-9]+";